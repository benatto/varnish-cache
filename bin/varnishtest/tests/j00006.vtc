varnishtest "Code coverage basic UNIX jail"

feature user_varnish
feature group_varnish
feature root

server s1 {
	rxreq
	txresp
} -start

varnish v1 \
	-jail "-jlinux,user=varnish,ccgroup=varnish,mgt_filter=/tmp/mgt_filter" \
	-vcl+backend {
} -start

client c1 {
	txreq
	rxresp
	expect resp.status == 200
} -run
